  "engines": {
    "node": ">=8"
  },
  "bin": "bin/index.js",
  "dependencies": {
    "chalk": "^2.4.2",
    "commander": "^3.0.2",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "bin/**/*.js": [
      "npm run prettier",
      "git add"
